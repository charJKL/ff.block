{
	"compilerOptions":
	{
		"target": "es2022",
		"module": "ES2022",
		"moduleResolution": "bundler",
		"jsx": "react-jsx",
		
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"sourceMap": true,
		
		"paths":
		{
			"$src/*": ["./src/*"]
		}
	},
	"include": 
	[
		"./src/**/*"
	]
}